{
  "version": 2,
  "builds": [
    {
      "src": "npm run build-web",
      "use": "@vercel/node"
    },
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/name/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/coords/(.*)/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "APP_MODULE": "main:app"
  }
}